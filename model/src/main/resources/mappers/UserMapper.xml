<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mall.model.mapper.UserMapper">
    <resultMap id="BaseResult" type="com.mall.model.dto.user.User">
        <result property="userId" column="user_id" />
        <result property="userCode" column="user_code" />
        <result property="userName" column="userName" />
        <result property="userNickName" column="user_nick_name" />
        <result property="userPwd" column="user_pwd" />
        <result property="userPhone" column="user_phone" />
        <result property="userEmail" column="user_email" />
    </resultMap>
    <select id="selectUser" resultMap="BaseResult">
        select * from c_user
    </select>
    <select id="addUser" resultType="_int" parameterType="com.mall.model.dto.user.User"></select>
</mapper>